    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
    "@types/node": "~18.16.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "concurrently": "^9.1.2",
